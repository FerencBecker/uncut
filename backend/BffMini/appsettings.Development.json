{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "DataPaths": {
    "Studios": "../../data/sample/studios",
    "Images": "../../data/sample/images",
    "Manifests": "../../data/sample/manifests"
  },
  "FileService": {
    "DataPath": "../../data",
    "CacheEnabled": false,
    "CacheDurationMinutes": 5
  }
}
